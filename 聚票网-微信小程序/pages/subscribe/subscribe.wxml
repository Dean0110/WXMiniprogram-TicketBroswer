<!--pages/subscribe/subscribe.wxml-->
<view class="subscribe">
    <view class="subscribe_item" 
    wx:for="{{subscribeList}}" wx:key="{{index}}">
        <view class="img">
            <image mode="widthFix" src="{{item.img}}"/>
        </view>
        <view class="info">
            <text class="name">{{item.name}}</text>
            <text class="desc">订阅数：{{item.subscribeNum}}</text>
        </view>
    </view>
</view>
